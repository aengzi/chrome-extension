{
  "name": "BJ 앵지",
  "version": "1.1.0",
  "description": "[BJ 앵지] 방송시청, 영상다운로드, 커뮤니티",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self';",
  "permissions": [
    "gcm",
    "http://101.79.136.27/*",
    "http://101.79.136.36/*",
    "http://211.110.86.208/*",
    "http://*.aengzi.com/*",
    "http://*.api.aengzi.com/*",
    "http://*.afreecatv.com/*",
    "https://*.afreecatv.com/*",
    "https://*.google.com/*",
    "https://*.googleapis.com/*",
    "notifications",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.min.js"],
    "persistent": true
  },
  "content_scripts": [{
    "all_frames": true,
    "matches": ["https://www.youtube.com/embed/*"],
    "css": ["main/assets/youtube_embed.css"]
  }],
  "browser_action": {
    "default_icon": {
      "16": "main/assets/img/aengzi_icon_16.jpg",
      "32": "main/assets/img/aengzi_icon_32.jpg",
      "48": "main/assets/img/aengzi_icon_48.jpg",
      "128": "main/assets/img/aengzi_icon_128.jpg"
    }
  },
  "icons": {
    "16": "main/assets/img/aengzi_icon_16.jpg",
    "32": "main/assets/img/aengzi_icon_32.jpg",
    "48": "main/assets/img/aengzi_icon_48.jpg",
    "128": "main/assets/img/aengzi_icon_128.jpg"
  },
  "manifest_version": 2
}
